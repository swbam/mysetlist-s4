{
  "name": "mysetlist",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "start": "cd apps/web && pnpm start",
    "lint": "biome check --diagnostic-level=error",
    "lint:fix": "biome check --write",
    "test": "turbo test",
    "typecheck": "turbo typecheck",
    "clean": "git clean -xdf node_modules .turbo .next",
    "db:push": "cd packages/database && pnpm push",
    "db:generate": "cd packages/database && pnpm generate",
    "db:migrate": "cd packages/database && pnpm migrate",
    "db:studio": "cd packages/database && pnpm studio",
    "seed": "tsx scripts/seed-database.ts",
    "seed:comprehensive": "tsx scripts/seed-comprehensive-data.ts",
    "sync:artists": "tsx scripts/sync-trending-artists.ts",
    "sync:popular": "tsx scripts/sync-popular-artists.ts",
    "check:env": "tsx scripts/check-env.ts",
    "deploy": "tsx scripts/deploy.ts",
    "deploy:preview": "vercel",
    "deploy:quick": "vercel --prod",
    "analyze": "cd apps/web && ANALYZE=true pnpm build"

  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@repo/typescript-config": "workspace:*",
    "@supabase/supabase-js": "^2.52.1",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "dotenv": "^16.6.1",
    "tsx": "^4.20.3",
    "turbo": "^2.5.3",
    "typescript": "^5.8.3",
    "vercel": "^44.5.0"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.13.1"
}
