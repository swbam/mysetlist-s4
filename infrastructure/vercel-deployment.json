{
  "name": "mysetlist-production",
  "version": 2,
  "public": false,
  "scope": "swbams-projects",
  "regions": ["iad1"],
  "build": {
    "env": {
      "NODE_ENV": "production",
      "NEXT_TELEMETRY_DISABLED": "1",
      "SKIP_BUILD_STATIC_GENERATION": "false"
    }
  },
  "functions": {
    "app/api/sync/**": {
      "maxDuration": 300,
      "memory": 1024,
      "runtime": "nodejs20.x"
    },
    "app/api/admin/**": {
      "maxDuration": 60,
      "memory": 512,
      "runtime": "nodejs20.x"
    },
    "app/api/search/**": {
      "maxDuration": 30,
      "memory": 256,
      "runtime": "nodejs20.x"
    },
    "app/api/trending/**": {
      "maxDuration": 30,
      "memory": 256,
      "runtime": "nodejs20.x"
    },
    "app/api/health/**": {
      "maxDuration": 15,
      "memory": 128,
      "runtime": "nodejs20.x"
    },
    "app/api/monitoring/**": {
      "maxDuration": 30,
      "memory": 256,
      "runtime": "nodejs20.x"
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://mysetlist-sonnet.vercel.app"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization, X-Requested-With"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=(), payment=()"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains; preload"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval' *.vercel.app *.sentry.io *.posthog.com; style-src 'self' 'unsafe-inline'; img-src 'self' data: https:; connect-src 'self' *.supabase.co *.spotify.com *.ticketmaster.com *.setlist.fm *.sentry.io *.posthog.com; font-src 'self' data:"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/admin",
      "destination": "/admin/dashboard",
      "permanent": false
    },
    {
      "source": "/health",
      "destination": "/api/health/comprehensive",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/monitoring/(.*)",
      "destination": "https://o4504609404448768.ingest.us.sentry.io/$1"
    },
    {
      "source": "/analytics/(.*)",
      "destination": "https://us.i.posthog.com/$1"
    }
  ],
    "env": {
      "NEXT_PUBLIC_APP_URL": "https://mysetlist-sonnet.vercel.app",
      "NEXT_PUBLIC_APP_ENV": "production",
      "NODE_ENV": "production",
      "NEXT_PUBLIC_ENABLE_ANALYTICS": "true",
      "NEXT_PUBLIC_ENABLE_REALTIME": "true",
      "NEXT_PUBLIC_ENABLE_PERFORMANCE_MONITORING": "true",
      "CACHE_TTL_DEFAULT": "3600",
      "CACHE_TTL_STATIC": "86400",
      "CACHE_TTL_API": "300",
      "PERFORMANCE_BUDGET_LCP": "2500",
      "PERFORMANCE_BUDGET_FCP": "1800",
      "PERFORMANCE_BUDGET_CLS": "0.1",
      "API_TIMEOUT": "30000",
      "ENABLE_GZIP": "true",
      "ENABLE_BROTLI": "true",
      "DATABASE_URL": "postgresql://postgres.yzwkimtdaabyjbpykquu:Bambseth1590@aws-0-us-east-1.pooler.supabase.com:5432/postgres",
      "DIRECT_URL": "postgresql://postgres.yzwkimtdaabyjbpykquu:Bambseth1590@aws-0-us-east-1.pooler.supabase.com:5432/postgres",
